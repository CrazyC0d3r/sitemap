{
	"name": "welshpaul/sitemap",
	"type": "phpbb-extension",
	"description": "SEO Sitemap creates the XML sitemap index and listings",
	"homepage": "https://github.com/CrazyC0d3r/sitemap",
	"version": "1.0.2",
	"keywords": ["phpbb", "extension", "seo", "sitemap"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Paul Norman (WelshPaul)",
			"homepage": "https://www.ukvoipforums.com",
			"role": "Developer"
		},
		{
			"name": "Jeff Cocking (LotusJeff)",
			"role": "Original Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "SEO Sitemap",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<3.3@dev"
		},
		"version-check": {
			"host": "www.ukvoipforums.com",
			"directory": "/updatecheck/phpbb32/ext",
			"filename": "sitemap.json",
			"ssl": true
		}
	}
}
